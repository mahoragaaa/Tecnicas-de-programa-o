public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Carro meucarro = new Carro("ferrari","812 competizione",2021);//construtor
		
		meucarro.setAno(2012); // setter do ano
		System.out.println(meucarro.getModelo()); // getter do modelo
		String marca = meucarro.getMarca(); // getter da marca pegando o retorno
		System.out.println(marca); // impress√£o da variavel
		meucarro.ligar(); //chama metodo ligar
		meucarro.acelerar(); //chama metodo acelerar
		
	}

}
